const int leftButton = 6;
const int selectButton = 7;
const int rightButton = 8;

int menuType = -1; // -1 = Title 0 = Menu 1 = Any menu
int menuPosition = 0; // 0 = pulse; 1 = Rep; 2 = Steps; 3 = Location

void setup() {
  Serial.begin(9600); 
  u8g2.begin();
  u8g2.setFont(u8g2_font_5x7_tr);
  pinMode(leftButton, INPUT_PULLUP);
  pinMode(selectButton, INPUT_PULLUP);
  pinMode(rightButton , INPUT_PULLUP);
}

void loop() {
  u8g2.setBitmapMode(false);
  u8g2.firstPage();
  u8g2.drawStr(0, 10, "Date");
  u8g2.drawStr(110, 10, "Time");

  int leftButtonClicked = digitalRead(leftButton);
  int selectButtonClicked = digitalRead(selectButton);
  int rightButtonClicked = digitalRead(rightButton);

  if(!leftButtonClicked)
  {

    if(menuType == 0)
    {
        menuPosition--;
    }
    else if(menuType == -1)
    {
      menuType++;
    }
    else
    {
      menuType--;
    }
    
    if(menuPosition < 0)
    {
      menuPosition = 3;
    }
    
  }
  
  if(!rightButtonClicked)
  {
    if(menuType == 0)
    {
        menuPosition++;
    }
    else if(menuType == -1)
    {
      menuType++;
    }
    else {
      menuType--;
    }
    
    
    if(menuPosition > 3)
    {
      menuPosition = 0;
    }
    
  }

  if(!selectButtonClicked)
  {
    switch(menuType)
    {
      case -1:
        menuType++;
        break;
      case 0:
        menuType++;
        break;
      case 1:
        // resets data of current menu
        break;
      
    }
  }

  do {

    if(menuType == -1)
    {
      u8g2.drawStr(42, 32, "Mega Watch");
    }
    else if(menuType == 0)
    {
        u8g2.drawXBMP(0, 12, 128, 64, epd_bitmap_allArray[menuPosition]);
    }
    else if(menuType = 1)
    {
        // Selects module
    }
    
    
    
  }while (u8g2.nextPage());
  
  
  
  

}

